                </tr>
                <tr>
                    <td align="center">15:30 </td>
                    <td align="center" height="35"></td>
                    <td align="center"></td>
                    <td align="center"></td>
                    <td align="center"></td>
                </tr>
                <tr class="dif">
                    <td align="center">16:10 </td>
                    <td align="center" height="35"></td>
                    <td align="center"></td>
                    <td align="center"></td>
                    <td align="center"></td>
                </tr>
            </table>
        </div>


        <div class="date-picker">
            <div class="selected-date"></div>

            <div class="dates">
                <div class="month">
                    <div class="arrows prev-mth">&lt;</div>
                    <div class="mth"></div>
                    <div class="arrows next-mth">&gt;</div>
                </div>

                <div class="days"></div>
            </div>
        </div>

        <div class="text-box">
            <div class="hora">
                <h4>Hora</h4>
                <input type="text" name="Hora" size="8" />
            </div>
            <div class="tipo">
                <h4>Tipo de Compromisso</h4>
                <input type="text" name="Tipo" size="23" />
            </div>
            <div class="nome">
                <h4>Nome do Paciente</h4>
                <input type="text" name="Nome" size="30" />
                <button type="submit">&#128269;</button>
            </div>
            <div class="convenio">
                <h4>Convênio</h4>
                <select id="inputConvenio" class="form-control " name="convenio">
                    <option selected></option>
                    <option> Cassi</option>
                    <option> Particular</option>
                    <option> Part desc.</option>
                </select>
            </div>
            <div class="valor">
                <h4>Valor</h4>
                <input type="text" name="Valor" size="9" />
            </div>
            <div class="status">
                <h4>Status</h4>
                <select id="inputStatus" class="form-control " name="statud">
                    <option selected></option>
                    <option>Atendido</option>
                    <option>Aguardando</option>
                    <option>Nao Compareceu</option>
                </select>
            </div>
            <div class="chegada">
                <h4>Chegada</h4>
                <input type="text" name="Chegada" size="12" />
            </div>
            <div class="atendido">
                <h4>Atendido</h4>
                <input type="text" name="Atendido" size="12" />
            </div>
            <div class="notas">
                <h4>Notas</h4>
                <textarea name="notas" cols="50" rows="6"></textarea>
            </div>

        </div>
    </div>

    <script src="script.js"></script>
</body>

</html>
